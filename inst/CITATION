citHeader("To cite ezcox in publications use:")

citEntry(
  entry    = "Article",
  title    = "The predictive power of tumor mutational burden in lung cancer immunotherapy response is influenced by patients’ sex",
  author   = personList(as.person("Shixiang Wang"),
                       as.person("Jing Zhang"),
                       as.person("Zaoke He"),
                       as.person("Xue-Song Liu")),
  journal  = "International journal of cancer",
  year     = 2019,
  url      = "https://www.ncbi.nlm.nih.gov/pubmed/30972745",
  textVersion = paste(
   "Wang, Shixiang, et al. \"The predictive power of tumor mutational burden in lung cancer immunotherapy response is influenced by patients’ sex.\" International journal of cancer (2019)."
  )
)
